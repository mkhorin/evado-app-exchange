<div class="account-block">
    <% if (!_controller.user.isGuest()) { %>
    <div class="money-block">
        <span>$</span>
        <b title="My money" data-t-title="" data-handler="MoneyTitle">-</b>
    </div>
    <% } %>

    <div class="lot-toggle" data-action="lots" data-t="" title="Active lots">Lots</div>

    <div class="user-toggle">
        <div class="dropdown-toggle" title="Account" data-bs-toggle="dropdown">
            <span class="icon fa fa-user"></span>
        </div>
        <div class="dropdown-menu dropdown-menu-end">
            <a class="dropdown-item" href="#" data-action="myStocks" data-t="">My stocks</a>
            <a class="dropdown-item" href="#" data-action="myLots" data-t="">My lots</a>
        </div>
    </div>
</div>